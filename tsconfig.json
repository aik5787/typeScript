{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["cypress"],
    "outDir": "./dist"
  },
  "include": [
    "cypress/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
